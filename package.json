{
  "name": "@skills/skills-client-vue",
  "version": "1.0.0",
  "description": "Vue client reporting and display libraries for skills clients",
  "main": "dist/skills-client-vue.umd.min.js",
  "scripts": {
    "clean": "rm -rfv dist",
    "build": "npm run clean && rollup -c",
    "watch": "npm run clean && rollup -c --watch",
    "test": "npx jest --passWithNoTests",
    "test-watch": "npx jest --passWithNoTests --watch",
    "prepublishOnly": "npm run clean && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.evoforge.org:skills/skills-client-report-vue.git"
  },
  "author": "Skills Team",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@rollup/plugin-alias": "2.2.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-vue": "5.2.3",
    "jest": "24.9.0",
    "rollup": "1.26.3",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-terser": "5.1.2",
    "rollup-plugin-uglify": "6.0.3",
    "rollup-plugin-vue": "5.1.2",
    "vue": "2.6.10",
    "vue-jest": "3.0.5",
    "vue-template-compiler": "2.6.10"
  },
  "dependencies": {
    "@skills/skills-client-configuration": "1.0.0",
    "@skills/skills-client-js": "1.0.0",
    "@skills/skills-client-reporter": "1.0.0",
    "axios": "0.19.0",
    "axios-auth-refresh": "1.0.7",
    "lodash": "4.17.15",
    "postmate": "1.5.2"
  },
  "files": [
    "dist",
    "*"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**src/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text",
      "html",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
