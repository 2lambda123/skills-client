{
  "name": "@skills/skills-client-js",
  "version": "1.1.1",
  "license": "Apache-2.0",
  "description": "SkillTree Client Lib - Native JS Lib",
  "main": "dist/skills-display-js.umd.min.js",
  "scripts": {
    "lint": "npx eslint src/*",
    "clean": "rm -rf dist",
    "licenseDepCheck": "license-checker --production --onlyAllow='MIT;ISC;Apache-2.0' --summary",
    "licenseHeaderCheck": " npx license-check-and-add check -f license-add/license-add-config.json",
    "licenseHeaderAdd": " npx license-check-and-add add -f license-add/license-add-config.json",
    "licenseCheck": "npm run licenseHeaderCheck && npm run licenseDepCheck ",
    "build": "npm run licenseCheck && npm run test && npm run clean && npx rollup -c",
    "deploy-to-examples": "npm run build && cp ./dist/skills-display-js.umd.min.js ../skills-examples/skills-example-client-js/app/assets/js/@skills/skills-client-js/dist",
    "test": "NODE_ENV=test jest --coverage",
    "test-watch": "npx jest --passWithNoTests --coverage --watch",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "http://10.113.80.244/repository/skills-registry/"
  },
  "author": "SkillTree Team",
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "axios-mock-adapter": "1.17.0",
    "babel-jest": "24.9.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "license-check-and-add": "3.0.4",
    "license-checker": "25.0.1",
    "rollup": "1.23.1",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "5.1.2"
  },
  "dependencies": {
    "@skills/skills-client-configuration": "1.1.0",
    "@skills/skills-client-reporter": "1.1.1",
    "axios": "0.19.0",
    "postmate": "1.5.1"
  }
}
