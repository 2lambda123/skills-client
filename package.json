{
  "name": "@skills/skills-client-js",
  "version": "0.0.7",
  "description": "userSkills viewing and reporting library",
  "main": "dist/index.js",
  "scripts": {
    "lint": "npx eslint src/*",
    "babel": "npx babel src/* --out-dir dist",
    "clean": "rm -rf dist",
    "umd": "npx umd --commonJS SkillsDisplayJS dist/SkillsDisplayJS.js dist/SkillsDisplayJS.umd.js && rm dist/SkillsDisplayJS.js",
    "build": "npm run test && npm run clean && npm run lint && npm run babel && npm run umd && npm rm dist/SkillsDisplayJS.js && npx uglifyjs dist/SkillsDisplayJS.umd.js -c -m -o dist/SkillsDisplayJS.umd.min.js",
    "test": "NODE_ENV=test jest --coverage",
    "prepublish": "npm run build"
  },
  "author": "User Skills Team",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "babel-jest": "24.9.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "uglify-js": "3.6.0",
    "umd": "3.0.3"
  },
  "dependencies": {
    "@skills/skills-client-configuration": "0.0.10",
    "@skills/skills-client-reporter": "0.0.26",
    "axios": "0.19.0",
    "postmate": "1.5.1"
  }
}
