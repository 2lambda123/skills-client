    "scripts": {
        "licenseDepCheck": "license-checker --production --onlyAllow='MIT;ISC;Apache-2.0;0BSD' --summary",
        "licenseHeaderCheck": " npx license-check-and-add check -f license-add/license-add-config.json",
        "licenseHeaderAdd": " npx license-check-and-add add -f license-add/license-add-config.json",
        "licenseCheck": "npm run licenseHeaderCheck && npm run licenseDepCheck ",
        "ng": "ng",
        "start": "ng serve",
        "serve": "ng serve",
        "build": "npm run licenseCheck && npm run prepare && ng build --configuration production",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "prepare": "cd projects/skilltree/skills-client-ng && npm run prepare && cd -"
    },
