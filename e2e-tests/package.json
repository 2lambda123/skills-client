{
  "name": "e2e-tests",
  "version": "1.0.0",
  "description": "Skills Client Libs End-to-End tests",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:open:dev:vue": "cypress open --config baseUrl=http://localhost:8091",
    "cy:open:dev:react": "cypress open --config baseUrl=http://localhost:8083",
    "examples:start": "java -Dspring.config.location=classpath:/application.properties,file:serverConfigs/examples_application.properties -jar ./bin/skills-example-service-1.0-SNAPSHOT.jar",
    "examples:start:release": "java -Dspring.config.location=classpath:/application.properties,file:serverConfigs/examples_application.properties -jar ./target/servicesJars/skills-example-service-*.jar",
    "examples:waitToStart": "npx wait-on -t 40000 http://localhost:8090",
    "backend:start": "java  -Dspring.config.location=classpath:/application.yml,file:serverConfigs/backend_application.properties -jar ../../skills-service/backend/target/backend-*.jar",
    "backend:start:ci": "java  -Dspring.config.location=classpath:/application.yml,file:serverConfigs/backend_application.properties -Dlogging.level.org.springframework.web.socket=TRACE -jar ../skills-service/backend-*.jar",
    "backend:start:release": "java  -Dspring.config.location=classpath:/application.yml,file:serverConfigs/backend_application.properties -jar ./bin/backend-toTest.jar",
    "backend:waitToStart": "npx wait-port -v -t 60000 localhost:8080",
    "backend:resetDb": "java -cp ./db/h2-1.4.200.jar org.h2.tools.RunScript -url jdbc:h2:tcp://localhost:9090/mem:skills -user sa -script ./db/reset.sql",
    "release": "release-it --dry-run"
  },
  "author": "Skills Team",
  "license": "Apache 2",
  "devDependencies": {
    "cypress": "^3.8.2",
    "release-it": "^12.4.3",
    "wait-on": "3.3.0",
    "wait-port": "0.2.7"
  }
}
